{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/favicon\\.(ico|png|svg)",
      "dest": "/favicon.$1"
    },
    {
      "src": "/rainmakers-logo-small\\.png",
      "dest": "/rainmakers-logo-small.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
